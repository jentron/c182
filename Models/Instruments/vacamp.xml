<?xml version="1.0"?>

<PropertyList>

    <path>vacamp.ac</path>
    
<animation>
  <type>scale</type>
  <x-offset>0.58</x-offset>
  <y-offset>0.58</y-offset>
  <z-offset>0.58</z-offset>
 </animation>
    
    <animation>
        <type>material</type>
        <object-name>Face</object-name>
        <object-name>VacNeedle</object-name>
        <object-name>AmpNeedle</object-name>
  <emission>
   <red>1.0</red>
   <green>0.8</green>
   <blue>0.4</blue>
   <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
  </emission>
    </animation>
    
    

    <animation>
  <type>rotate</type>
  <object-name>VacNeedle</object-name>
  <property>systems/vacuum/suction-inhg</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>0 </dep></entry>     
   <entry><ind> 3.0</ind><dep> -8.77 </dep></entry>     
   <entry><ind> 4.0</ind><dep> -33.80 </dep></entry>     
   <entry><ind> 5.0</ind><dep>  -61.12</dep></entry>     
   <entry><ind> 6.0</ind><dep>  -88.55 </dep></entry>     
   <entry><ind> 7.00</ind><dep> -112.93 </dep></entry>     
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
    <center>
      <x-m> 0.001 </x-m>
      <y-m> -0.029 </y-m>
      <z-m> -0 </z-m>
    </center>
 </animation>
    
   <animation>
        <type>rotate</type>
        <object-name>AmpNeedle</object-name>
        <property>systems/electrical/amps</property>
 <interpolation>
      <entry><ind>60</ind><dep> 55.85</dep></entry>
      <entry><ind>45</ind><dep>42.96</dep></entry>
      <entry><ind>30</ind><dep>29.15 </dep></entry>
      <entry><ind>15</ind><dep>14.63 </dep></entry>
      <entry><ind>0</ind><dep>0 </dep></entry>
      <entry><ind>-15</ind><dep>-14.63</dep></entry>
      <entry><ind>-30</ind><dep>-29.15</dep></entry>
      <entry><ind>-45</ind><dep>-42.96</dep></entry>
      <entry><ind>-60</ind><dep>-55.85</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0.001 </x-m>
      <y-m> 0.036 </y-m>
      <z-m> 0 </z-m>
    </center>
    </animation>

   

</PropertyList>
